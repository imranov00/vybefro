{
  "expo": {
    "name": "vybefro",
    "slug": "vybefro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "statusBar": {
      "style": "light",
      "backgroundColor": "transparent",
      "translucent": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imranow.vybefro",
      "statusBar": {
        "style": "light"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.imranow.vybefro",
      "statusBar": {
        "backgroundColor": "transparent",
        "translucent": true
      },
      "navigationBar": {
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0b44d60e-c7c4-4986-9c94-afcb2bbbbc59"
      }
    },
    "owner": "imranow",
    "updates": {
      "url": "https://u.expo.dev/0b44d60e-c7c4-4986-9c94-afcb2bbbbc59"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
